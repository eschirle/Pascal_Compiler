program equalities;

begin
    if 1>=1 then writeln(0) else writeln(1);
    if 1<=1 then writeln(0) else writeln(1);
    if 1=1 then writeln(0) else writeln(1);
    if 1<>0 then writeln(0) else writeln(1);
    if 1<>1 then writeln(1) else writeln(0);
    if 1<0 then writeln(1) else writeln(0);
    if 1<=0 then writeln(1) else writeln(0);
    if 1>=0 then writeln(0) else writeln(1);
    if 1>0 then writeln(0) else writeln(1);
end.
